{
  "name": "heroicons-animated-react-native",
  "version": "1.0.0",
  "description": "Demo app showcasing 316 beautifully animated Heroicons for React Native",
  "main": "expo-router/entry",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heroicons-animated/heroicons-animated-react-native.git"
  },
  "author": "heroicons-animated",
  "license": "MIT",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "generate-icons": "npx tsx scripts/generate-icons.ts",
    "build:package": "cd packages/react-native && pnpm build"
  },
  "dependencies": {
    "@heroicons-animated/react-native": "workspace:*",
    "expo": "~54.0.33",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "@biomejs/biome": "^1.9.0",
    "@types/react": "~19.1.0",
    "tsx": "^4.19.0",
    "typescript": "~5.9.3"
  }
}
